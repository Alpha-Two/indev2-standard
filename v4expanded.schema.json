{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "$defs": {
        "cell": {
            "type": "object",
            "oneOf": [
                {
                    "properties": {
                        "devID": { "$ref": "#/$defs/ID" },
                        "modInternalID": { "$ref": "#/$defs/ID" },
                        "modExternalID": { "$ref": "#/$defs/ID" },
                        "cellShortID": { "$ref": "#/$defs/ID" }
                    }
                },
                {
                    "properties": {
                        "marker": {
                            "type": "string"
                        }
                    }
                }
            ],
            "required": ["modExternalID", "cellShortID"]
        },
        "ID": {
            "type": "integer",
            "minimum": 0,
            "exclusiveMaximum": 18446744073709551616
        }
    },
    "properties": {
        "modList": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "referenceAddress": {
                        "oneOf": [
                            {
                                "type": "string",
                                "pattern": "^https?:\\/\\/\\w+\\.\\w+((\\/\\w*)*)$"
                            },
                            {
                                "type": "array",
                                "prefixItems": [
                                    { "$ref": "#/$defs/ID" },
                                    { "$ref": "#/$defs/ID" }
                                ]
                            }
                        ]
                    },
                    "permissions": {
                        "type": "string",
                        "pattern": "^[AD][NS][NO]$"
                    },
                    "config": {
                        "type": "object"
                    }
                },
                "required": ["referenceAddress"]
            }
        },
        "coreDisabled": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "referenceAddress": { "$ref": "#/$defs/ID" }
                }
            }
        },
        "cellOrder": {
            "type": "object",
            "properties": {
                "beforeGenerator": { "$ref": "#/$defs/cell" },
                "afterGenerator": { "$ref": "#/$defs/cell" },
                "beforeCW": { "$ref": "#/$defs/cell" },
                "afterCW": { "$ref": "#/$defs/cell" },
                "beforeCCW": { "$ref": "#/$defs/cell" },
                "afterCCW": { "$ref": "#/$defs/cell" },
                "beforeMover": { "$ref": "#/$defs/cell" },
                "afterMover": { "$ref": "#/$defs/cell" }
            },
            "required": [
                "beforeGenerator",
                "afterGenerator",
                "beforeCW",
                "afterCW",
                "beforeCCW",
                "afterCCW",
                "beforeMover",
                "afterMover"
            ]
        },
        "levels": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "config": {
                        "type": "object"
                    },
                    "height": {
                        "type": "integer",
                        "minimum": 0
                    },
                    "width": {
                        "type": "integer",
                        "minimum": 0
                    },
                    "levelArray": {
                        "type": "array",
                        "items": {
                            "type": "array",
                            "items": {
                                "$ref": "#/$defs/cell"
                            }
                        }
                    }
                },
                "required": ["height", "width", "levelArray"]
            }
        }
    },
    "required": ["cellArray"]
}
